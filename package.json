  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbmusso/gremlin-loader/issues"
  },
  "homepage": "https://github.com/jbmusso/gremlin-loader#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-node5": "^11.0.1",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0"
  },
  "dependencies": {
    "loader-utils": "^0.2.13",
    "lodash": "^4.7.0"
  }
